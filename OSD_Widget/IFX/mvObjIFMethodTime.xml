<?xml version="1.0" encoding="UTF-8" ?>

<node name="/mv/mvapp/time">	
	<interface name="mv.mvapp.time">		
		<method name="GetWorkTime">			
			<arg name="hours" type="i" direction="out" />
		</method>		
		<method name="SetWorkTime">			
			<arg name="hours" type="i" direction="in" />	
		</method>	
		<signal name='SigSetWorkTime'>
			<arg name='hours' type='i' />
		</signal>	
		
		<method name="GetCalibTime">
			<annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="stTIME_INFO"/>			
			<arg name="time" type="(iiiiii)" direction="out" />
			<doc>
          		<line>time = struct(uiSec,uiMin,uiHour,uiMday,uiMon,uiYear)</line>
          		<line>uiSec = uiSec</line>
          		<line>uiMin = uiMin</line>
          		<line>uiHour = uiHour</line>
          		<line>uiMday = uiMday</line>
			<line>uiMon = uiMon</line>
          		<line>uiYear = uiYear</line>
       			 </doc>
		</method>		
		<method name="SetCalibTime">
			<annotation name="org.qtproject.QtDBus.QtTypeName.In0" value="stTIME_INFO"/>		
			<arg name="time" type="(iiiiii)" direction="in" />
			<doc>
          		<line>time = struct(uiSec,uiMin,uiHour,uiMday,uiMon,uiYear)</line>
          		<line>uiSec = uiSec</line>
          		<line>uiMin = uiMin</line>
          		<line>uiHour = uiHour</line>
          		<line>uiMday = uiMday</line>
			<line>uiMon = uiMon</line>
          		<line>uiYear = uiYear</line>
       			 </doc>		
		</method>
		<signal name='SigSetCalibTime'>
			<annotation name="org.qtproject.QtDBus.QtTypeName.In0" value="stTIME_INFO"/>
			<annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="stTIME_INFO"/>
			<arg name='time' type='(iiiiii)' />
			<doc>
          		<line>time = struct(uiSec,uiMin,uiHour,uiMday,uiMon,uiYear)</line>
          		<line>uiSec = uiSec</line>
          		<line>uiMin = uiMin</line>
          		<line>uiHour = uiHour</line>
          		<line>uiMday = uiMday</line>
			<line>uiMon = uiMon</line>
          		<line>uiYear = uiYear</line>
       			 </doc>
		</signal>	
		
	</interface>

</node>
